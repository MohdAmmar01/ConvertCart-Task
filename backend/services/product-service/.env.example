# .env.example for product-service

# Port number the product service will run on
PORT=3001

# MongoDB connection URI
MONGODB_URI=mongodb://localhost:27017/convertcart

# WooCommerce API base URL (replace with your Woo store URL)
WOO_BASE_URL=https://your-woocommerce-site.com

# WooCommerce API credentials (replace with actual credentials when creating .env)
WOO_CONSUMER_KEY=your_consumer_key
WOO_CONSUMER_SECRET=your_consumer_secret

# Frontend React App URL (used for CORS or redirects)
REACT_URL=http://localhost:3000
